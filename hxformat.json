{
  "indentation": {
    "character": "    ",
    "indentCaseLabels": true
  },
  "lineEnds": {
    "leftCurly": "after",
    "emptyCurly": "noBreak",
    "objectLiteralCurly": {
      "leftCurly": "after"
    },
    "typedefCurly": {
      "leftCurly": "after"
    }
  },
  "sameLine": {
    "ifElse": "same",
    "doWhile": "next",
    "tryBody": "next",
    "tryCatch": "next"
  },
  "wrapping": {
    "arrayMatrixWrap": "matrixWrapWithAlign",
    "arrayWrap": {
      "defaultWrap": "fillLine",
      "defaultLocation": "afterLast"
    },
    "maxLineLength": 100
  },
  "whitespace": {
    "typeHintColonPolicy": "after"
  }
}
